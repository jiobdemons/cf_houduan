<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cjcwecha.cjcwecha.dao.AccountDao">
    <!-- 根据主键查询-->
    <select id="getById" resultType="com.cjcwecha.cjcwecha.entity.AccountEntity" parameterType="java.lang.Integer" >
        select  *  from account
        where id = #{id}
    </select>
</mapper>